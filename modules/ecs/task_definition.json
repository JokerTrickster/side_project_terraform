{
    "family": "dev_frog",
    "containerDefinitions": [
        {
            "image": "__REPO_DOMAIN__/__REPO_URL__@__IMAGE_DIGEST__",
            "name": "__ENV___frog",
            "cpu": 2048,
            "memoryReservation": 512,
            "environment": [
                {
                    "name": "PROJECT",
                    "value": "frog"
                },
                {
                    "name": "PORT",
                    "value": "80"
                },
                {
                    "name": "ENV",
                    "value": "__ENV__"
                },
                {
                    "name": "REGION",
                    "value": "__REGION__"
                },
                {
                    "name": "IS_LOCAL",
                    "value": "false"
                },
                {
                    "name": "MYSQL_USER",
                    "value": "frog"
                },
                {
                    "name": "MYSQL_PASSWORD",
                    "value": "examplepassword"
                },
                {
                    "name": "MYSQL_HOST",
                    "value": "devstg_mongodb.internal_ap-northeast-2"
                },
                {
                    "name": "MYSQL_PORT",
                    "value": "3306"
                },
                {
                    "name": "MYSQL_DATABASE",
                    "value": "frog_dev"
                }
            ],
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "ecs-idea-__ENV___frog",
                    "awslogs-region": "__REGION__"
                }
            },
            "ulimits": [
                {
                    "name": "nofile",
                    "softLimit": 65535,
                    "hardLimit": 65535
                }
            ],
            "linuxParameters": {
                "maxSwap": 0,
                "swappiness": 0
            },
            "mountPoints": [],
            "volumesFrom": [],
            "essential": true,
            "disableNetworking": false,
            "privileged": false
        }
    ],
    "taskRoleArn": "arn:aws:iam::190009833917:role/idea_ecsTask___ENV___frog___REGION__",
    "executionRoleArn": "arn:aws:iam::190009833917:role/idea_ecsTaskExec___ENV___frog___REGION__",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "EC2"
    ]
}